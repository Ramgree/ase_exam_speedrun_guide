<%= form_for @game_changeset, Routes.game_path(@conn, :create), fn game -> %>

<div class="form-group">
  <%= label game, :username, class: "control-label" %>
  <%= text_input game, :username, id: "username", class: "form-control" %>
  <%= error_tag game, :username %>
</div>

<div class="form-group">
  <%= submit "Submit", id: "Submit", class: "btn btn-primary" %>
</div>

<% end %>
